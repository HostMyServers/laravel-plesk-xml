{
    "name": "nickurt/laravel-plesk-xml",
    "description": "Plesk Onyx 17.8 Xml for Laravel 5.8",
    "keywords": ["plesk", "plesk onyx", "plesk onyx 17.8", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "laravel/framework": "5.8.*",
        "guzzlehttp/guzzle": "6.*",
        "spatie/array-to-xml": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.5"
    },
    "autoload": {
        "psr-4": {
            "nickurt\\PleskXml\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "nickurt\\PleskXml\\ServiceProvider"
            ],
            "aliases": {
                "PleskXml": "nickurt\\PleskXml\\Facade"
            }
        }
    }
}
